{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/peterschussheim/gamma"
  },
  "scripts": {
    "start": "gamma start",
    "start:verbose": "DEBUG=*,-babel*,-eslint*,-express:* gamma start",
    "start:inspect": "gamma start --inspect-brk=127.0.0.1:9229",
    "build": "gamma build",
    "deploy": "now && now alias",
    "clean": "rm -rf node_modules && rm -rf yarn.lock",
    "test": "yarn run jest -- --runInBand --watch",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "precise-commits": "precise-commits",
    "precommit": "yarn run precise-commits",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "webpack-defaults": "webpack-defaults",
    "eject": "react-scripts eject"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.@(json|md|ts)": [
      "yarn prettier --write",
      "git add"
    ],
    "*.@(js)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "6.24.1",
    "cross-env": "5.2.0",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "gamma-core": "0.0.1",
    "husky": "0.14.3",
    "lerna": "^3.0.0-rc.0",
    "lint-staged": "7.2.0",
    "precise-commits": "1.0.2",
    "prettier": "1.13.5",
    "prop-types": "15.6.1",
    "react-hot-loader": "4.3.4"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.2.2",
    "apollo-client": "2.3.2",
    "apollo-link": "1.2.2",
    "apollo-link-error": "1.1.0",
    "apollo-link-http": "1.5.4",
    "apollo-link-persisted-queries": "0.2.1",
    "apollo-link-retry": "2.2.4",
    "apollo-link-state": "0.4.1",
    "apollo-link-ws": "1.0.8",
    "apollo-upload-client": "8.1.0",
    "apollo-utilities": "1.0.13",
    "babel-plugin-emotion": "9.2.6",
    "common-tags": "1.8.0",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-fetch": "2.2.2",
    "css": "2.2.3",
    "css.escape": "1.5.1",
    "emotion": "9.2.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "helmet": "3.13.0",
    "hpp": "0.2.2",
    "passport": "0.4.0",
    "prisma-binding": "2.1.3",
    "query-string": "6.1.0",
    "react": "^16.4.0",
    "react-apollo": "2.1.4",
    "react-dom": "^16.4.0",
    "react-emotion": "9.2.3",
    "react-error-boundary": "1.2.3",
    "react-helmet-async": "0.1.0-alpha",
    "react-loadable": "5.5.0",
    "react-router-dom": "4.3.1",
    "shared": "0.0.1",
    "toobusy-js": "0.5.1",
    "ws": "6.0.0"
  }
}
