{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/peterschussheim/gamma"
  },
  "scripts": {
    "start:prod": "NODE_ENV=production DEBUG=*,-babel*,-eslint*,-express:*,-compression* node build/server.js",
    "inspect-prod": "NODE_ENV=production node --inspect-brk build/server.js",
    "start": "gamma start -r now-env",
    "start:verbose": "DEBUG=*,-babel*,-eslint*,-express:*,-compression*,-send* gamma start -r now-env",
    "start:inspect": "gamma start -r now-env --inspect-brk=127.0.0.1:9229",
    "build": "gamma build",
    "deploy": "now && now alias",
    "clean": "rm -rf node_modules && rm -rf yarn.lock",
    "test": "yarn run jest -- --runInBand --watch",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "precise-commits": "precise-commits",
    "precommit": "yarn run precise-commits",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "webpack-defaults": "webpack-defaults",
    "eject": "react-scripts eject"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.@(json|md|ts)": [
      "yarn prettier --write",
      "git add"
    ],
    "*.@(js)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-plugin-emotion": "9.2.11",
    "babel-plugin-transform-class-properties": "6.24.1",
    "cross-env": "5.2.0",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "gamma-core": "0.0.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "precise-commits": "1.0.2",
    "prettier": "1.14.3"
  },
  "dependencies": {
    "@emotion/core": "10.0.0-beta.6",
    "apollo-cache-inmemory": "1.3.8",
    "apollo-client": "2.4.5",
    "apollo-link": "1.2.3",
    "apollo-link-error": "1.1.1",
    "apollo-link-http": "1.5.5",
    "apollo-link-persisted-queries": "0.2.1",
    "apollo-link-retry": "2.2.5",
    "apollo-link-state": "0.4.2",
    "apollo-link-ws": "1.0.9",
    "apollo-upload-client": "8.1.0",
    "apollo-utilities": "1.0.13",
    "common-tags": "1.8.0",
    "connect-redis": "3.3.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-fetch": "2.2.2",
    "css": "2.2.3",
    "css.escape": "1.5.1",
    "debug": "4.1.0",
    "emotion": "9.2.12",
    "emotion-server": "9.2.12",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fast-json-stable-stringify": "2.0.0",
    "formik": "1.3.1",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "helmet": "3.13.0",
    "hiredis": "0.5.0",
    "hpp": "0.2.2",
    "ioredis": "4.2.0",
    "now-env": "3.1.0",
    "passport": "0.4.0",
    "prisma-binding": "2.1.3",
    "prop-types": "15.6.2",
    "query-string": "6.1.0",
    "react": "^16.6.0",
    "react-apollo": "2.2.4",
    "react-dom": "^16.6.0",
    "react-emotion": "9.2.12",
    "react-error-boundary": "1.2.3",
    "react-helmet-async": "0.2.0",
    "react-loadable": "5.5.0",
    "react-router-dom": "4.3.1",
    "shared": "0.0.1",
    "subscriptions-transport-ws": "0.9.15",
    "toobusy-js": "0.5.1",
    "ws": "6.1.0",
    "yup": "0.26.6"
  }
}
