{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/peterschussheim/gamma"
  },
  "scripts": {
    "start:prod": "NODE_ENV=production DEBUG=*,-babel*,-eslint*,-express:*,-compression*,-connect-redis,-body-parser,-express-session node build/server.js",
    "inspect-prod": "NODE_ENV=production node --inspect-brk build/server.js",
    "start": "gamma start -r now-env",
    "start:verbose": "DEBUG=*,-babel*,-eslint*,-express:*,-compression*,-send*,-ioredis:*,-connect-redis,-body-parser,-express-session gamma start -r now-env",
    "start:inspect": "gamma start -r now-env --inspect-brk=127.0.0.1:9229",
    "build": "gamma build",
    "schema": "apollo-codegen introspect-schema http://localhost:4000/api --output ./src/schema.json",
    "deploy": "now && now alias",
    "types": "apollo-codegen generate ./src/queries/index.ts --addTypename --schema=./schema.json --target typescript --output ./src/__generated__/types.ts",
    "clean": "rm -rf node_modules && rm -rf yarn.lock",
    "test": "gamma test --env=jsdom",
    "lint": "eslint .",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "webpack-defaults": "webpack-defaults"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.12",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "4.3.1",
    "@types/webpack-env": "1.13.6",
    "apollo-codegen": "0.20.2",
    "babel-plugin-emotion": "10.0.7",
    "babel-plugin-transform-class-properties": "6.24.1",
    "emotion-ts-plugin": "0.3.0",
    "fork-ts-checker-webpack-plugin": "0.5.1",
    "gamma-core": "0.0.1",
    "ts-jest": "23.10.5",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.2.4",
    "worker-plugin": "3.0.0"
  },
  "dependencies": {
    "@babel/parser": "7.3.1",
    "@emotion/cache": "10.0.7",
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@peterschussheim/monaco-editor": "0.15.61",
    "@reach/menu-button": "0.1.7",
    "@rebass/grid": "6.0.0-7",
    "apollo-cache-inmemory": "1.3.8",
    "apollo-client": "2.4.5",
    "apollo-link": "1.2.3",
    "apollo-link-error": "1.1.1",
    "apollo-link-http": "1.5.5",
    "apollo-link-persisted-queries": "0.2.1",
    "apollo-link-retry": "2.2.5",
    "apollo-link-state": "0.4.2",
    "apollo-link-ws": "1.0.9",
    "apollo-upload-client": "8.1.0",
    "apollo-utilities": "1.0.13",
    "body-parser": "1.18.3",
    "classnames": "2.2.6",
    "common-tags": "1.8.0",
    "connect-redis": "3.3.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "cross-fetch": "2.2.2",
    "css": "2.2.3",
    "css.escape": "1.5.1",
    "debug": "4.1.0",
    "emotion": "10.0.7",
    "emotion-theming": "10.0.7",
    "escape-string-regexp": "1.0.5",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fast-json-stable-stringify": "2.0.0",
    "formik": "1.4.2",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "hiredis": "0.5.0",
    "hpp": "0.2.2",
    "idb-keyval": "3.1.0",
    "ioredis": "4.2.0",
    "json-to-ast": "2.1.0",
    "lodash": "4.17.11",
    "now-env": "3.1.0",
    "passport": "0.4.0",
    "prettier": "1.16.1",
    "prisma-binding": "2.1.3",
    "prop-types": "15.6.2",
    "query-string": "6.1.0",
    "raf": "3.4.1",
    "react": "^16.7.0",
    "react-apollo": "2.3.1",
    "react-dom": "^16.7.0",
    "react-error-boundary": "1.2.3",
    "react-helmet": "5.2.0",
    "react-icons": "3.2.2",
    "react-router-dom": "4.3.1",
    "react-split-pane": "0.1.85",
    "react-tippy": "1.2.3",
    "recast": "0.16.2",
    "semver": "5.6.0",
    "shared": "0.0.1",
    "subscriptions-transport-ws": "0.9.15",
    "toobusy-js": "0.5.1",
    "ts-loader": "5.3.1",
    "validate-npm-package-name": "3.0.0",
    "vscode-icons-js": "7.25.2",
    "ws": "6.1.2",
    "yup": "0.26.6"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "gamma-core/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "gamma-core/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
