{
  "extends": ["tslint:latest", "tslint-react", "../../tslint.json"],
  "linterOptions": {
    "exclude": [
      "src/components/MonacoEditor/vendor/eslint.bundle.js",
      "./build/react-loadable.json"
    ]
  },
  "rules": {
    "no-implicit-dependencies": false,
    "prefer-const": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "no-var-keyword": false,
    "prefer-for-of": false,
    "no-console": false
  }
}
