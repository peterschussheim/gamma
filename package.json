{
  "name": "graphql-api-server",
  "version": "0.0.0",
  "main": "src/index.js",
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d lib/",
    "start": "cross-env NODE_ENV=production nodemon src/index.js --watch src -e js,graphql babel-node",
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=*,-snapdragon:*,-nodemon*,-babel nodemon -r dotenv/config --watch src -e js,graphql ./node_modules/.bin/babel-node src/index.js",
    "inspect": "cross-env NODE_ENV=development cross-env DEBUG=*,-snapdragon:*,-nodemon*,-babel nodemon -r dotenv/config --watch src -e js,graphql ./node_modules/.bin/babel-node src/index.js --inspect",
    "test": "cross-env NODE_ENV=test jest --watch",
    "lint": "eslint .",
    "flow": "flow",
    "seed": "knex seed:run",
    "migrate": "knex migrate:latest",
    "precommit": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-register": "6.26.0",
    "cross-env": "5.1.5",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.46.3",
    "eslint-plugin-prettier": "2.6.0",
    "flow-bin": "0.72.0",
    "husky": "0.14.3",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "prettier": "1.12.1",
    "pretty-quick": "1.4.1",
    "sqlite3": "4.0.0"
  },
  "dependencies": {
    "apollo-engine": "1.1.1",
    "apollo-server": "2.0.0-beta.1",
    "apollo-server-express": "2.0.0-beta.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "connect-redis": "3.3.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dataloader": "1.4.0",
    "debug": "3.1.0",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-async-errors": "2.1.2",
    "express-session": "1.15.6",
    "graphql": "0.13.2",
    "graphql-import": "0.5.2",
    "graphql-tools": "3.0.1",
    "helmet": "3.12.0",
    "hpp": "0.2.2",
    "ioredis": "3.2.2",
    "jsonwebtoken": "8.2.1",
    "keygrip": "1.0.2",
    "knex": "0.14.6",
    "node-uuid": "1.4.8",
    "now-env": "3.0.4",
    "passport": "0.4.0",
    "passport-github2": "0.1.11",
    "pg": "7.4.3",
    "pg-connection-string": "2.0.0",
    "raven": "2.6.1",
    "request-promise": "4.2.2",
    "supports-color": "5.4.0"
  }
}
