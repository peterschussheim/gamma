{
  "name": "graphql-api-server",
  "version": "0.0.0",
  "main": "src/index.js",
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d lib/",
    "start": "cross-env NODE_ENV=production nodemon src/index.js --watch src -e js,graphql babel-node",
    "dev": "cross-env NODE_ENV=development cross-env DEBUG=*,-body-parser:*,-express:router:*,-snapdragon:*,-nodemon*,-babel nodemon -r dotenv/config --watch src -e js,graphql ./node_modules/.bin/babel-node src/index.js",
    "inspect": "cross-env NODE_ENV=development cross-env DEBUG=*,-body-parser:*,-express:router:*,-snapdragon:*,-nodemon*,-babel nodemon -r dotenv/config --watch src -e js,graphql ./node_modules/.bin/babel-node src/index.js --inspect",
    "clean": "rm -rf node-modules",
    "gen-schema-types": "./node_modules/.bin/babel-node src/utils/createTypes.js",
    "lint": "eslint .",
    "flow": "flow",
    "test": "cross-env NODE_ENV=test jest --watch",
    "test:setup": "rm test.sqlite3 || true; knex migrate:latest --env test && knex seed:run --env test",
    "migrate": "knex migrate:latest",
    "seed": "cross-env DEBUG=knex knex seed:run",
    "precommit": "pretty-quick --staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "cross-env": "5.1.6",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.0",
    "faker": "4.1.0",
    "graphql-cli": "2.16.0",
    "husky": "0.14.3",
    "jest": "^23.0.0",
    "nodemon": "^1.17.5",
    "prettier": "1.12.1",
    "pretty-quick": "1.6.0"
  },
  "dependencies": {
    "apollo-engine": "1.1.1",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "connect-redis": "3.3.3",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "dataloader": "1.4.0",
    "debug": "3.1.0",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "graphql-yoga": "1.14.6",
    "helmet": "3.12.1",
    "hpp": "0.2.2",
    "jsonwebtoken": "8.2.1",
    "keygrip": "1.0.2",
    "node-uuid": "1.4.8",
    "now-env": "3.0.4",
    "passport": "0.4.0",
    "passport-github2": "0.1.11",
    "raven": "2.6.2",
    "redis": "2.8.0",
    "request-promise": "4.2.2",
    "supports-color": "5.4.0"
  }
}
