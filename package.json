{
  "name": "gamma",
  "version": "0.0.0",
  "private": true,
  "author": "Peter Schussheim <peter@schussheim.com> (https://peterschussheim.com/)",
  "license": "MIT",
  "bolt": {
    "workspaces": [
      "packages/*"
    ]
  },
  "repository": {
    "url": "https://github.com/peterschussheim/gamma"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node build-ssr/main.js",
    "start:api": "cross-env NODE_ENV=production node build-api/main.js",
    "dev:web": "cross-env NODE_PATH=./ react-app-rewired start",
    "dev:api": "cd api && yarn dev",
    "dev:ssr": "cross-env NODE_PATH=./ cross-env NODE_ENV=development cross-env DEBUG=build*,ssr*,shared:middlewares* cross-env DIR=ssr backpack",
    "build": "npm run build:web && npm run build:ssr",
    "prebuild:api": "rimraf build-api",
    "build:api": "cross-env NODE_PATH=./ cross-env DIR=api backpack build",
    "postbuild:api": "cp api/package.json build-api/package.json && cp now.json build-api/now.json",
    "prebuild:ssr": "rimraf build-ssr",
    "build:ssr": "cross-env NODE_PATH=./ cross-env DIR=ssr backpack build",
    "postbuild:ssr": "cp now.json build-ssr/now.json",
    "build:web": "cross-env NODE_PATH=./ react-app-rewired build",
    "deploy": "now && now alias",
    "clean": "rm -rf node_modules",
    "jest": "jest",
    "test": "npm run jest -- --runInBand --watch",
    "test:ci": "npm run jest -- --forceExit --outputFile test-results.json --json --maxWorkers=2",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "precise-commits": "precise-commits",
    "precommit": "npm run precise-commits",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "webpack-defaults": "webpack-defaults",
    "eject": "react-scripts eject",
    "deprecated-dev:api": "cross-env NODE_PATH=./ cross-env NODE_ENV=development cross-env DEBUG=build*,api*,shared:middlewares* cross-env DIR=api backpack"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.@(json|md|ts)": [
      "yarn prettier --write",
      "git add"
    ],
    "*.@(js)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@peterschussheim/backpack-core": "0.9.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-inline-import-graphql-ast": "^2.0.4",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bundle-buddy-webpack-plugin": "0.3.0",
    "crossenv": "0.0.2-security",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "precise-commits": "1.0.2",
    "prettier": "1.13.5",
    "prop-types": "15.6.1",
    "raven": "2.6.3",
    "raw-loader": "0.5.1",
    "react-app-rewire-hot-loader": "1.0.1",
    "react-app-rewired": "1.5.2",
    "react-hot-loader": "4.3.4",
    "react-scripts": "1.1.4",
    "rimraf": "2.6.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "webpack-bundle-analyzer": "2.13.1",
    "write-file-webpack-plugin": "4.3.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.2.2",
    "apollo-client": "2.3.2",
    "apollo-link": "1.2.2",
    "apollo-link-error": "1.1.0",
    "apollo-link-http": "1.5.4",
    "apollo-link-persisted-queries": "0.2.1",
    "apollo-link-state": "0.4.1",
    "apollo-link-ws": "1.0.8",
    "apollo-utilities": "1.0.13",
    "cross-env": "5.2.0",
    "emotion": "9.2.3",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "offline-plugin": "5.0.5",
    "react": "^16.4.0",
    "react-apollo": "2.1.4",
    "react-dom": "^16.4.0",
    "react-emotion": "9.2.3",
    "react-error-boundary": "1.2.3",
    "react-loadable": "5.4.0",
    "react-router-dom": "4.3.1",
    "subscriptions-transport-ws": "0.9.9"
  }
}
